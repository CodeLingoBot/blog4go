<blog4go minlevel="info">
	<filter levels="trace">
		<rotatefile path="trace.log" type="time"></rotatefile>
	</filter>
	<filter levels="debug" colored="true">
		<file path="debug.log"></file>
	</filter>
	<filter levels="info" colored="true">
		<console></console>
	</filter>
	<filter levels="warn,error">
		<rotatefile path="error.log" type="size" rotateSize="50000000"></rotatefile>
	</filter>
	<filter levels="critical">
		<socket network="udp" address="127.0.0.1:12124"></socket>
	</filter>
</blog4go>
